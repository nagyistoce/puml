#include<stdio.h>
#include<stdlib.h>
intmakearg(char*s,char**args[]);

intmain()
{
char**argv,str[100];
intargc,m=0,i=0,k=-1;
printf("%s","Entertext>");
while(str[k]!='\n')
{
k++;
str[k]=getchar();
}

argc=makearg(str,&argv);
printf("\ntheamountofarguementsare%d\n\n",argc);


while(m<argc)
{
printf("%s",argv[m]);
printf("\n");
m++;
}
}

intmakearg(char*s,char**args[])
{
inti=0,tok=0,flag=0,row=0,col=0;

char**tmp=(char**)malloc(1000*(sizeof(char*)));
if(tmp=='\0')
{
return-1;
}

while(s[i]!='\0')
{
if(s[i]!=''&&flag==0)
{
tmp[col]=&s[i];
flag=1;
}
elseif(s[i]==''&&flag==1)
{
tmp[col][row]='\0';
row=0;
col++;
flag=0;
tok++;
}
i++;
if(s[i]=='\n')
{
s[i]='\0';
}


if(flag==1)
{
row++;
}
}
tok++;
*args=tmp;
returntok;
}
